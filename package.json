{
  "name": "chatter-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"rsw watch\" \"vite --open --mode wasm\"",
    "build": "rsw build && tsc && vite build --mode wasm",
    "devser": "concurrently --names 'vite, server' --kill-others 'vite --host 0.0.0.0 --open --mode server' 'cargo run --bin self_server'",
    "buildser": "tsc && vite build --mode server",
    "dev-tauri": "vite --mode tauri",
    "build-tauri": "tsc && vite build --mode tauri",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tycheck": "tsc -b",
    "check": "run-p tycheck lint",
    "preview": "vite preview",
    "buildghp": "rsw build && tsc && vite build --mode ghpage",
    "shuttle": "run-p buildser && cargo shuttle run",
    "tauri": "tauri",
    "rsw": "rsw"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/vite": "^4.1.7",
    "@tauri-apps/api": "^1.6.0",
    "axios": "^1.9.0",
    "hox": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "src-wasm": "link:src-wasm/pkg",
    "use-immer": "^0.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@tauri-apps/cli": "^1.6.3",
    "@types/node": "^20.17.50",
    "@types/react": "^18.3.22",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitejs/plugin-react-swc": "^3.10.0",
    "concurrently": "^8.2.2",
    "daisyui": "^5.0.37",
    "eslint": "^9.27.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.1.0",
    "npm-run-all2": "^6.2.6",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1"
  }
}
