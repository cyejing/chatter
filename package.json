{
  "name": "chatter-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"rsw watch\" \"vite --open --mode wasm\"",
    "build": "rsw build && tsc && vite build --mode wasm",
    "devser": "concurrently --names 'vite, server' --kill-others 'vite --host 0.0.0.0 --open --mode server' 'cargo run --bin self_server'",
    "buildser": "tsc && vite build --mode server",
    "dev-tauri": "vite --mode tauri",
    "build-tauri": "tsc && vite build --mode tauri",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "buildghp": "rsw build && tsc && vite build --mode ghpage",
    "shuttle": "run-p buildser && cargo shuttle ",
    "tauri": "tauri",
    "rsw": "rsw"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.1.3",
    "@tauri-apps/api": "^1.5.1",
    "axios": "^1.6.8",
    "hox": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.11",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "daisyui": "^4.10.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.3.0"
  }
}
